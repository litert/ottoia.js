            "port": 4483,
            "address": "127.0.0.128",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/cli/**/*.js",
                "${workspaceFolder}/cli/*.js",
                "${workspaceFolder}/lib/**/*.js",
                "${workspaceFolder}/lib/*.js"
            ]
        }
